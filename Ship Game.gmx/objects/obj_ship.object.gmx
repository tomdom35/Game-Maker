            audio_play_sound(snd_boost,10,false);
        }
        thrusting = true;
        speed = 9;
        tail = instance_create(x,y,obj_boost_tail);
        tail.image_angle=image_angle;
    }
    else{
        speed=6;
        thrusting = false;
    }
    sprite_index=spr_shipMovingBoost;
}
else{
   if(speed&gt;0)
    speed-=air_resistance;
    sprite_index=spr_ship;   
}

if(keyboard_check(ord('A'))){
    direction+=speed;
    image_angle+=speed;
    if(keyboard_check(ord('W'))&amp;&amp;!obj_energy_bar.burnOut)
        sprite_index=spr_shipTurningLeftBoost;
    else
        sprite_index=spr_shipTurningLeft;
}
if(keyboard_check(ord('D'))){
    direction-=speed;
    image_angle-=speed;
    if(keyboard_check(ord('W'))&amp;&amp;!obj_energy_bar.burnOut)
        sprite_index=spr_shipTurningRightBoost;
    else
        sprite_index=spr_shipTurningRight;
}
if(keyboard_check_pressed(vk_up)){
    if((!shot1||!shot2||!shot3)){
        if(trackingShot==0)
            bullet = instance_create(x,y,obj_bullet);
        else{
            bullet = instance_create(x,y,obj_tracking_bullet);
            trackingShot--;
        }
        bullet.image_angle=image_angle;
        bullet.direction=direction;
        bullet.speed=20;
        audio_sound_pitch(snd_shoot,0.8+random(.5));
        audio_play_sound(snd_shoot,10, false);
    }
    if(!shot1){
        shot1=true;
        alarm[0] = 30
    }
    else if(!shot2){
        shot2=true;
        alarm[1] = 30;
    }
    else if(!shot3){
        shot3=true;
        alarm[2] = 30;
    }
}

if(keyboard_check_pressed(vk_down)&amp;&amp;canPlaceMine){
    canPlaceMine = false;
    instance_create(x,y, obj_orb_mine);
    alarm[3] = 600;
}

if(keyboard_check_pressed(ord('W'))&amp;&amp;!obj_energy_bar.burnOut)
