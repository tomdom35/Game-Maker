            <string>if(room==rm_menu){
    if(keyboard_string!=""){
        input = keyboard_string
    }
    if(real(input)==1||real(input)==0||real(input)&gt;=10){
        input = "10"
    }
    else if(real(input)&lt;2){
        input = "2"
    }
    keyboard_string = ""
    //if(!(real(input)&lt;=10 || real(input)&gt;=2)&amp;&amp; input!="") input = "2"
    draw_text(40,(room_height/2+100), "Enter board size: " + input + " by " + input)
    if(keyboard_check_pressed(vk_enter)){
        size = real(input)
        if(size&lt;=10 &amp;&amp; size&gt;=2){
            room_set_width(rm_game_board, (size+2)*64)
            room_set_height(rm_game_board, (size+2)*64)
            window_set_size((size+2)*64,(size+2)*64)
            room_goto_next()
        }
    }
    miniBoard=((real(input)-1)*20)/2
    xStart = (room_width/2) - (miniBoard)
    yStart=(((room_height-544)/2)+544)- miniBoard - 25
    
    for(i=0;i&lt;=real(input);i++){
        for(j=0;j&lt;=real(input);j++){
            instance_create(xStart+(i*20),yStart+(j*20),obj_menu_dot)
        }
    }
    
    if(keyboard_check_pressed(vk_down)){
        multiplayer = true
        singlePlayer = false
    }
    else if(keyboard_check_pressed(vk_up)){
        singlePlayer = true
        multiplayer = false
    }
}
</string>
